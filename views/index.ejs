<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/jquery-ui.css' />

    <link rel="icon" type="image/x-icon"  href="/images/favicon.png">
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="/js/1-11-4-jquery-ui.js"></script>
    <script src="/js/jq-ui.min.js"></script>

    <script src="/socket.io/socket.io.js"></script>
    <script src="knockout-3.4.0.js"></script>
    <!--<script src="/js/chart.min.js"></script>-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/1.0.2/Chart.js"></script>
    <script src="/js/socket_handle.js"></script>
    <script type="text/javascript" src="/js/handle.js"></script>
    <script src="/js/mousewheel.js"></script>
    <script src="/js/scroll.js"></script>

    <link rel='stylesheet' href='/stylesheets/scroll.css' />
  </head>


  <body>

  <div id="logo"></div>
  <div id="u_menu" class="work_style">
    <input type="button" class="u_button" id="button_home" ws="1" title="Логи">
    <input type="button" class="u_button" id="button_table" ws="2" title="Бизнес-статистика">
    <input type="button" class="u_button" id="button_camera" ws="3" title="Камеры наблюдения">
    <input type="button" class="u_button" id="button_weight" ws="4" title="Карта линии">
  </div>
  <div id="wrapper">
    <div id="workspace_1" class="work_style">
      <div id="logs">тут будут логи</div>
    </div>
    <div id="workspace_2" class="work_style">
      <div id="charts">
        <section>
          <article>
            <canvas id="myChart" width="744" height="625">
            </canvas>
          </article>
        </section>
      </div>
    </div>
    <div id="workspace_3" class="work_style">
      <div id="videos">а здесь видео с камер</div>
    </div>
    <div id="workspace_4" class="work_style">
      <div id="linemap">а здесь карта линий</div>
    </div>
    <div id="io-status-box" class="work_style">
      <div class="box-header">
        <label class="box-header-label">IO Status</label>
        <hr class="box-header-hr">
      </div>
      <div>
        <% for(var i = 0; i < 20; i++){ %>
        <div class="box-inner">
          <div class="box-inner-block">
            <div class="box-inner-text">
              <label class="box-inner-label">I/O-<%= i %>: </label>
            </div>
            <div class="box-inner-switches">
              <div class="switch">
                <input id="cmn-toggle-<%= i %>" class="cmn-toggle cmn-toggle-round-flat" type="checkbox">
                <label for="cmn-toggle-<%= i %>"></label>
              </div>
              <div class="led">
                <div class="led_light"></div>
              </div>
            </div>
          </div>
        </div>
        <% } %>
      </div>
    </div>
  </div>

  </body>
</html>
